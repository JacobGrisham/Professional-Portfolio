{
  "name": "project_portfolio",
  "version": "1.0.0",
  "description": "Portfolio of web development projects",
  "main": "app.js",
  "scripts": {
    "app": "node app.js",
    "watch:sass": "node-sass public/sass/main.scss public/css/style.css -w",
    "compile:sass": "node-sass public/sass/main.scss public/css/style.compile.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' public/css/style.compile.css -o public/css/style.prefix.css",
    "compress:css": "node-sass public/css/style.prefix.css public/css/style.css --output-style compressed",
    "build": "npm-run-all compile:sass prefix:css compress:css bundle",
    "start": "npm-run-all --parallel app watch:sass"
  },
  "author": "Jacob Grisham",
  "license": "Copyright Notice and Statement: currently not offering any license. Permission only to view and download.",
  "devDependencies": {
    "autoprefixer": "^9.8.6",
    "jest": "^26.6.3",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.2"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "helmet": "^3.23.3",
    "lax.js": "^2.0.3"
  }
}
